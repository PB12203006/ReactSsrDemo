{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel _dev:*",
    "_dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "_dev:build-server": "webpack --config webpack.server.js --mode=development --watch",
    "_dev:build-client": "webpack --config webpack.client.js --mode=development --watch",
    "one-off-build-dev": "npm-run-all --serial _one-off-dev:*",
    "_one-off-dev:build-client": "webpack --config webpack.client.js --mode=development",
    "_one-off-dev:build-server": "webpack --config webpack.server.js --mode=development"
  },
  "author": "",
  "license": "ISC",
